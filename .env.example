# InfluxDB 設定
INFLUXDB_USERNAME=admin
INFLUXDB_PASSWORD=your_secure_password_here
INFLUXDB_ORG=fitlog
INFLUXDB_BUCKET=health_data
INFLUXDB_ADMIN_TOKEN=your_admin_token_here
INFLUXDB_URL=http://localhost:8086

# Grafana 設定
GRAFANA_ADMIN_PASSWORD=your_grafana_password_here

# Google Fit API 設定
GOOGLE_FIT_CLIENT_ID=your_client_id_here
GOOGLE_FIT_CLIENT_SECRET=your_client_secret_here

# データ取得設定
FETCH_DAYS_BACK=1
TIMEZONE=Asia/Tokyo